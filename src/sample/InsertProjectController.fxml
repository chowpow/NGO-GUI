<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #f0932b;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InsertProjectController">
   <children>
      <Text layoutX="198.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Insert Project" textAlignment="CENTER" wrappingWidth="203.21630859375">
         <font>
            <Font name="Tahoma" size="23.0" />
         </font>
      </Text>
      <GridPane hgap="15.0" layoutX="74.0" layoutY="91.0" prefHeight="250.0" prefWidth="452.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TextField fx:id="p_id" prefWidth="179.0" promptText="project ID (6 digits)" />
            <TextField fx:id="p_description" promptText="project description" GridPane.columnIndex="1" />
            <TextField fx:id="p_budget" promptText="project budget (max 7 figures)" GridPane.rowIndex="1" />
            <TextField fx:id="p_duration" promptText="project duration (months and days)" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="insertButton" layoutX="310.0" layoutY="325.0" mnemonicParsing="false" onAction="#insertProject" prefHeight="50.0" prefWidth="113.0" text="Insert">
